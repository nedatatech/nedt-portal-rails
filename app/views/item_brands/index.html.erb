<p id="notice"><%= notice %></p>

<h1>Item Brands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @item_brands.each do |item_brand| %>
      <tr>
        <td><%= item_brand.name %></td>
        <td><%= item_brand.description %></td>
        <td><%= link_to 'Show', item_brand %></td>
        <td><%= link_to 'Edit', edit_item_brand_path(item_brand) %></td>
        <td><%= link_to 'Destroy', item_brand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item Brand', new_item_brand_path %>
