<h1>New England Data Tech - Portal</h1> 
<h1>------------------------------</h1>
<h1>Orders</h1> 
<br>
<% if !logged_in? %>
  <h1>Log In</h1>
  <%= form_tag login_path do %>
    <div class="field">
      <%= label_tag :username %><br />
      <%= text_field_tag :username, params[:username] %>
    </div>
    <div class="field">
      <%= label_tag :password %><br />
      <%= password_field_tag :password %>
    </div>
    <div class="actions"><%= submit_tag "Log In" %></div>
  <% end %>
<% else %>
  <%= button_to "Pending",  orders_pending_path, :method => :get %>
  <%= button_to "Completed", orders_completed_path, :method => :get %>
  <%= button_to "List all", orders_path, :method => :get %>
  <br>
  <%= button_to "Order Settings", "/pages/inventory_settings", :method => :get %>
<% end %>

